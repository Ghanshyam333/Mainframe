\par }\pard \ql \li0\ri0\widctlpar\brdrt\brdrs\brdrw45\brsp20 \brdrl\brdrs\brdrw45\brsp80 \brdrb\brdrs\brdrw45\brsp20 \brdrr\brdrs\brdrw45\brsp80 \faauto\adjustright\rin0\lin0\rtlgutter\itap0 {\f1\fs20\insrsid8600455 01\tab EMPLOYEE-REC.
\par \tab 05   EMP-NAME.
